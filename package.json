{
  "name": "nl-parser",
  "version": "1.0.1",
  "description": "A Node.js parser for Naturelink GPS devices, decoding raw hex data and extracting location, event, and IO information.",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "node ./src/index.js",
    "test": "mocha ./test --recursive",
    "lint": "eslint ./src ./test --ext .js",
    "lint:fix": "eslint ./src ./test --ext .js --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npm run lint:fix && npm run prettier:fix",
    "example": "node ./examples/basic.js",
    "prepare": "npm run lint && npm run test",
    "prepublishOnly": "npm run format && npm run test"
  },
  "keywords": [
    "naturelink",
    "gps",
    "parser",
    "nodejs",
    "iot",
    "telemetry",
    "binary",
    "protocol",
    "nl01",
    "nl02"
  ],
  "author": "Rado HT Simarmata <simarmatarado@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/radosimarmata/naturelink-parser.git"
  },
  "bugs": {
    "url": "https://github.com/radosimarmata/naturelink-parser/issues"
  },
  "homepage": "https://github.com/radosimarmata/naturelink-parser#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dayjs": "^1.11.9"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0"
  }
}
