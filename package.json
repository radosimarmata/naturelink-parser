{
  "name": "naturelink-parser",
  "version": "1.0.0",
  "description": "A Node.js parser for Naturelink GPS devices, decoding raw hex data and extracting location, event, and I/O information.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/index.js",
    "test": "mocha ./test --recursive",
    "lint": "eslint ./src ./test --ext .js",
    "lint:fix": "eslint ./src ./test --ext .js --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npm run lint:fix && npm run prettier:fix",
    "example": "node ./examples/basic.js"
  },
  "keywords": [
    "naturelink",
    "gps",
    "parser",
    "nodejs",
    "iot",
    "telemetry"
  ],
  "author": "Rado HT Simarmata <simarmatarado@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dayjs": "^1.11.9"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radosimarmata/naturelink-parser.git"
  },
  "bugs": {
    "url": "https://github.com/radosimarmata/naturelink-parser/issues"
  },
  "homepage": "https://github.com/radosimarmata/naturelink-parser#readme"
}
